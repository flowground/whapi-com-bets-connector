{
    "type": "object",
    "properties": {
        "apiSecret": {
            "required": true,
            "type": "string",
            "pattern": "^[a-zA-Z0-9]{19,50}$"
        },
        "apiTicket": {
            "required": true,
            "type": "string",
            "pattern": "^[a-zA-Z][a-zA-Z0-9-]{39,100}$"
        },
        "betId": {
            "required": true,
            "type": "string"
        },
        "cashInValue": {
            "required": true,
            "type": "number",
            "format": "double"
        },
        "cashinBetDelayId": {
            "required": true,
            "type": "string"
        }
    }
}